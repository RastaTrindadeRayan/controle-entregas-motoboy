{
  "appId": "com.motoboy.entregas",
  "appName": "Controle de Entregas",
  "webDir": "dist",
  "server": {
    "androidScheme": "https"
  },
  "android": {
    "allowMixedContent": true,
    "captureInput": true,
    "webContentsDebuggingEnabled": false
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 2000,
      "backgroundColor": "#f3f4f6",
      "showSpinner": false
    },
    "StatusBar": {
      "style": "DARK"
    }
  }
}
